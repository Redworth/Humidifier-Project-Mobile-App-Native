{
  "expo": {
    "name": "FirebaseNative",
    "slug": "FirebaseNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Redworth6(Current)(Transparent).png",
    "splash": {
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.redworth.redworth",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": "true"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.redworth.redworth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
